{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "description": "The Fastify boilerplate with all you need to build your SaaS, AI tool, or any other web app and make your first $ online fast.",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/main.ts",
    "lint": "prettier --write --config prettier.config.js --list-different src/**/*.ts",
    "start": "node dist/main.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/qs": "^6.14.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.6.0",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "mongodb": "^7.0.0",
    "qs": "^6.14.0"
  }
}
