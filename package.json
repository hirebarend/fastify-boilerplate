{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "description": "The Fastify boilerplate with all you need to build your SaaS, AI tool, or any other web app and make your first $ online fast.",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/main.js",
    "lint": "prettier --write --config prettier.config.js --list-different src/**/*.ts",
    "start": "node ./dist/main.js",
    "test": "tsc && jest --coverage --testPathPatterns ./dist/.*\\.test\\.js$"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/qs": "^6.14.0",
    "jest": "^30.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^17.2.2",
    "fastify": "^5.5.0",
    "mongodb": "^6.19.0",
    "qs": "^6.14.0"
  }
}
