    "fnv-plus": "^1.3.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.11.0",
    "qs": "^6.13.1",
    "tiny-secp256k1": "^2.2.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  }
}
