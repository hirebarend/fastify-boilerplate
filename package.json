{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "description": "The Fastify boilerplate with all you need to build your SaaS, AI tool, or any other web app and make your first $ online fast.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/main.js",
    "lint": "prettier --write --config prettier.config.js --list-different src/**/*.ts",
    "start": "node ./dist/main.js",
    "test": "tsc && jest --coverage --testPathPattern ./dist/.*\\.test\\.js$"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.17",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "mongodb": "^6.12.0",
    "qs": "^6.13.1"
  }
}
