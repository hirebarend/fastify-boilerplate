    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/qs": "^6.9.17",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.1.0",
    "axios": "^1.7.8",
    "bip32": "^5.0.0-rc.0",
    "bip39": "^3.1.0",
